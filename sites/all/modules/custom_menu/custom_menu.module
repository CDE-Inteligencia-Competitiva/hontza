function custom_menu_set_max_title_tabs($my_list,$is_tab=0){
    $result=$my_list;
    $max=custom_menu_define_grupo_title_max_len($is_tab);
    if(!empty($result)){
        foreach($result as $i=>$row){
            $len=strlen($row->title);
            //intelsat-2015
            $result[$i]->title_orig=$row->title;
            if($len>$max){
