            function get_grupo_custom_menu_href(grupo_href,href_value){
                var result="";
                var grupo_subdominio=get_subdominio_href(grupo_href);
                var href_subdominio=get_subdominio_href(href_value);
                var my_array;                   
                if(grupo_subdominio!=href_subdominio){
                    result=grupo_href;
                    my_array=href_value.split("/"); 
                    if(my_array.length>2){
                        my_array=my_array.slice(3);
                        red_destination=my_array.join("/");
                        result=result+"&red_destination="+red_destination;
                    }
                    return result;
                }
                return href_value;
            }
            function get_subdominio_href(href_value){
                var my_array=href_value.split("/");
                if(my_array.length>2){
                    return my_array[2];
                }
                return "";    
            }
            function get_grupo_string_by_grupo_href(grupo_string,grupo_href){
                return grupo_string;
                var base_url_konp=parse_url_js(grupo_href);
                var konp=base_url_konp.protocol+"//"+base_url_konp.host+"/";
                if(konp==base_url_js){
                    //alert("grupo_string="+grupo_string);
                    return grupo_string;
                }
                konp=base_url_konp.host;
                //alert("grupo_string="+grupo_string+"_"+konp);
                return grupo_string+"_"+konp;
            }
            function parse_url_js(url) {
                var a = document.createElement("a");
                  a.href = url;
                  return {
                    host: a.host,
                    hostname: a.hostname,
                    pathname: a.pathname,
                    port: a.port,
                    protocol: a.protocol,
                    search: a.search,
                    hash: a.hash
                  };
            }
        });';

