function custom_menu_get_href($row){
    $find='href="';
    $pos=strpos($row,$find);
    if($pos===FALSE){
        return '';
    }else{
        $url=substr($row,$pos+strlen($find));
        $pos2=strpos($url,'"');
        if($pos2===FALSE){
            return '';
        }else{
            $result=substr($url,0,$pos2);
            return $result;
        }    
    }
    return '';
}
