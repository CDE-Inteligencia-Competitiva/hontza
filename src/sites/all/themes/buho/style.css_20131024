ul.menu {
  padding: 0;
  margin: 0;
  list-style: none;
  list-style-image: none;
  border: solid 1px #ddd;
  /*border-bottom: 0;*/
}
ul.menu ul {
  width: 100%;
  margin: 0;
  padding: 0;
  border: none;
}
ul.menu ul ul {
  margin: 0;
  padding: 0;
}
ul.menu li,
ul.menu li.leaf,
ul.menu li.expanded,
ul.menu li.collapsed {
  list-style: none;
  list-style-image: none;
  list-style-type: none;
