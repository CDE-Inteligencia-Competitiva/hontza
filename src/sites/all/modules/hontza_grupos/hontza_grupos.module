function hontza_grupos_get_num_grupos_despues_de_borrar_grupo($grupo_nid,$grupo_nid_array){
    if(!empty($grupo_nid_array)){
        $result=0;
        foreach($grupo_nid_array as $i=>$v){
            if($grupo_nid!=$v){
                $result++;
            }
        }
        return $result;
    }
    return 0;
}
