    case 'insert':
      //Crear un vocabulario para cada grupo creado asociandolo.
      if ($node-> type=='grupo') {
         //create_grupo_chat($node);
		 $vocabulary = array(
            'name'  => $node->purl['value'],
            'description' => $node->og_description,
            'multiple'  => 1,
            'required'  => 0,
