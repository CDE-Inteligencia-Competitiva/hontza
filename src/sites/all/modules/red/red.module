}
function red_define_periodicidad_options(){
    $options=array();
    $res=db_query('SELECT * FROM {red_periodicidad} WHERE 1 ORDER BY label ASC');
    while($row=db_fetch_object($res)){
        $options[$row->value]=$row->label;
    }
    return $options;
}
