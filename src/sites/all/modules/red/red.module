function red_parse_str($data,&$result)
{
    $data = preg_replace_callback('/(?:^|(?<=&))[^=[]+/', function($match) {
        return bin2hex(urldecode($match[0]));
    }, $data);
    parse_str($data, $values);
    $result=array_combine(array_map('hex2bin', array_keys($values)), $values);
}
