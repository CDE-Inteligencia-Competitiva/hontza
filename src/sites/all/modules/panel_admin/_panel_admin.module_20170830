function panel_admin_get_edit_logo_url(){
    $result='admin/build/themes/settings/buho';
    if(defined('_IS_EDIT_LOGO_URL') && _IS_EDIT_LOGO_URL==1){
        if(defined('_EDIT_LOGO_URL')){
            return _EDIT_LOGO_URL;
        }
    }
    return $result;
}
