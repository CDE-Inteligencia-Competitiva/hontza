    'access callback'=>'panel_admin_crm_exportar_admin_access',        
    );
    $items['panel_admin/crm_exportar/listas/%/ver_clientes']=array(
    'title' => 'Clients',
    'page callback' => 'panel_admin_crm_exportar_listas_ver_clientes_callback',
    'access callback'=>'panel_admin_crm_exportar_admin_access',    
    );
     $items['panel_admin/crm_exportar/textos/exportar-csv/%/download']=array(
    'title'=>'Clients',
    'page arguments' => array(''),       
    'page callback' => 'panel_admin_crm_exportar_textos_exportar_csv_download_callback',       
    'access callback'=>'panel_admin_crm_exportar_admin_access',
  );
    $items['panel_admin/ayuda_popup/get_help_popup_node']=array(
    'title'=>'Help',
    'page callback' => 'panel_admin_ayuda_popup_get_help_popup_node_callback',       
    'access callback'=>TRUE,
  );
    $items['panel_admin/crm_exportar/types']=array(
    'title' => 'Types Management',
    'page callback' => 'panel_admin_crm_exportar_types_callback',
    'access callback'=>'panel_admin_crm_exportar_admin_access',        
    );
    $items['panel_admin/crm_exportar/types/create']=array(
    'title' => 'Create Type',
    'page arguments' =>array('panel_admin_crm_exportar_types_form'),
    'page callback'=>'drupal_get_form',
    'access callback'=>'panel_admin_crm_exportar_admin_access',        
    );
    $items['panel_admin/crm_exportar/types/%/edit']=array(
    'title' => 'Edit Type',
    'page arguments' =>array('panel_admin_crm_exportar_types_form'),
    'page callback'=>'drupal_get_form',
    'access callback'=>'panel_admin_crm_exportar_admin_access',        
    );
    $items['panel_admin/crm_exportar/types/%/delete']=array(
    'title' => 'Delete Type',
    'page arguments' =>array('panel_admin_crm_exportar_types_delete_form'),
    'page callback'=>'drupal_get_form',
    'access callback'=>'panel_admin_crm_exportar_admin_access',        
    );  
    return $items;
}
