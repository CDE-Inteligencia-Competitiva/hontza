function estrategia_block($op = 'list', $delta = 0, $edit = array()) {
 	switch ($op) {
    	case 'list':
 			//$blocks[400]['info'] = t('Gestionar estrategia');
                        $blocks[400]['info'] = t('Strategic Deployment');
                        $blocks[800]['info'] = t('Deploy Strategy');
			return $blocks;
